{
  "name": "react-simple-calendar",
  "version": "0.2.0",
  "description": "A minimal calendar component for react.",
  "main": "lib/index.js",
  "scripts": {
    "postinstall": "npm run lib",
    "dev": "concurrently \"npm run lib:watch\" \"npm run docs\"",
    "lib": "babel src/lib -d lib --ignore '**/__tests__'",
    "lib:watch": "babel src/lib -w -d lib --ignore '**/__tests__'",
    "docs": "webpack serve",
    "docs:prod": "webpack --mode production",
    "test": "jest"
  },
  "keywords": [
    "react",
    "calendar"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.2.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.6.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^23.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "sinon": "^6.3.5",
    "style-loader": "^0.21.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "author": "po8rewq",
  "homepage": "https://github.com/po8rewq/react-simple-calendar",
  "repository": {
    "type": "git",
    "url": "git@github.com:po8rewq/react-simple-calendar.git"
  },
  "jest": {
    "rootDir": "./src",
    "setupFilesAfterEnv": "./setupTests.js"
  }
}
